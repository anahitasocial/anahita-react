{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true,
    "navigator": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0],
    "react/no-danger": 0,
    "arrow-body-style": ["error", "always"]
  }
}
